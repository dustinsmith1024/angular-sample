html(lang="en", ng-app="myApp")
  head
    meta(charset='utf8')
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    base(href='/')
    title Angular Reporting App
    meta(name="description", content="")
    meta(name="keywords", content="")
    meta(name="author", content="")
    meta(name="viewport", content="width=device-width,initial-scale=1")
    link(rel='stylesheet/less', href='/less/styles.less')
    script(src="js/libs/less/less-1.3.0.min.js")
  body(ng-controller='AppCtrl', ng-cloak)
  
    div(class="navbar navbar-fixed-top")
      div(class="navbar-inner")
        div(class="container")
          a(class="btn btn-navbar", data-toggle="collapse", data-target=".nav-collapse")
            span(class="icon-bar")
            span(class="icon-bar")
            span(class="icon-bar")
          a(class="brand", href="#") Project name
          div(class="nav-collapse")
            ul(class="nav")
              li(class="active")
                a(href='view1') view1
              li
                a(href='tweets') tweets
              li
                a(href='names') names
              li
                a(href='reports') reports
    block content
    
    script(src="js/libs/jquery/jquery-1.7.2.min.js")
